@{
    ViewBag.Title = "Users";
}
@model IEnumerable<gammingStore.Models.User>
<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Full Name</th>
      <th>Email </th>
      <th>Role</th>
  </thead>
  @foreach (var item in Model){
    <tr>
      <td>@item.Username</td>
      <td>@item.FullName</td>
      <td>@item.Email</td>
      <td>@item.Role</td>
    </tr>
  }
</table>
