@{
  
}
@model List<gammingStore.Models.Order>
<div class="container">
<h1>Under Delivery</h1>
<div class= "table">
<table>
  <thead>
  <tr>
    <th>Id</th>
    <th>Price</th>
    <th>ProductName</th>
    <th>Quantity</th>
    <th>Delivered</th>
  </tr>
  </thead>
  <tbody>
@foreach(var item in Model){
  <tr>
    <td>@item.Id</td>
    <td>@item.Price</td>
    <td>@item.ProductName</td> 
    <td>@item.Quantity</td>
    <td>
      <a href="/Employees/Delivered/@item.Id"> Delivered? </a>
    </td>
  </tr>
}
</tbody>
</table>
</div>
</div>
