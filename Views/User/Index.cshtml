@{
  ViewBag.Title = User.Identity.Name;
}
@model IEnumerable<gammingStore.Models.Order>
<h1>
  welcome @User.Identity.Name
</h1>
<a href="/user/logout" class="btn btn-secondary">
  Logout
</a>
<h2>Orders</h2>
<table class="table">
  <thead>
    <tr>
      <th>Order Id</th>
      <th>Order Date</th>
      <th>Order Total</th>
      <th>Product Name</th>
      <th>Quantity</th>
    </tr>
  </thead>
  <tbody>
    @foreach(var order in Model){
      <tr>
        <td>@order.TranscationId</td>
        <td>@order.TranscationDate</td>
        <td>@order.Price</td>
        <td>@order.ProductName</td>
        <td>@order.Quantity</td>
      </tr>
    }
  </tbody>

