@{
if (User.Identity?.Name  == null){
  ViewBag.Title = "UNKNOWN";
}
else 
  ViewBag.Title = User.Identity.Name;
}
@model IEnumerable<gammingStore.Models.Order>
<div class="container">
  <div>
<h1>
  welcome @((User.Identity?.Name == null)? "UNKNOWN" : User.Identity?.Name)
</h1>
<a href="/user/logout" class="btn btn-secondary">
  Logout
</a>
  </div>
<div>
<h2>Orders</h2>
<div class="table">
<table >
  <thead>
    <tr>
      <th>Order Id</th>
      <th>Order Date</th>
      <th>Order Total</th>
      <th>Product Name</th>
      <th>Quantity</th>
    </tr>
  </thead>
  <tbody>
    @foreach(var order in Model){
      <tr>
        <td>@order.TranscationId</td>
        <td>@order.TranscationDate</td>
        <td>@order.Price</td>
        <td>@order.ProductName</td>
        <td>@order.Quantity</td>
      </tr>
    }
  </tbody>
</table>
</div>
</div>
<div>
  <a href="/User/EditProfile" class="btn btn-primary">Edit Profile</a>
</div>
</div>
